#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/nodejs/Projects/solinas-project/node_modules/.pnpm/needle@2.8.0/node_modules/needle/bin/needle/node_modules:/home/workspace/nodejs/Projects/solinas-project/node_modules/.pnpm/needle@2.8.0/node_modules/needle/bin/node_modules:/home/workspace/nodejs/Projects/solinas-project/node_modules/.pnpm/needle@2.8.0/node_modules/needle/node_modules:/home/workspace/nodejs/Projects/solinas-project/node_modules/.pnpm/needle@2.8.0/node_modules:/home/workspace/nodejs/Projects/solinas-project/node_modules/.pnpm/node_modules:/home/workspace/nodejs/Projects/solinas-project/node_modules:/home/workspace/nodejs/Projects/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/Projects/solinas-project/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/home/workspace/nodejs/Projects/solinas-project/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../needle@2.8.0/node_modules/needle/bin/needle" "$@"
else 
  exec node  "$basedir/../../../../../needle@2.8.0/node_modules/needle/bin/needle" "$@"
fi
